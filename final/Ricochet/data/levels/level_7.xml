<?xml version="1.0"?>
<root>
	<object type="bgcolor" r="5" g="40" b="28" a="255" />
	<!-- AXAMPLE AN EXPLANATION OF ATTRIBUTES
	<object 
	type="laser" 				// the type of object this will be
	position="-18.5,5" 			// the position of where the object will be located
	laser_direction="1,0" 		// the direction the laser is pointing(positive x is right, positive y is up. This direction will always be 0 degrees)
	moving="0"					// indicates whether the laser moves between 2 points (0=NotMoving, 1=Moving) 
	p1="0,0" 					// the two points the laser moves between (you always start at p1 and move toward p2)
	p2="5,0" 					//.. to start at point inbetween the 2 set the time variables
	duration="5" 				// how long it takes to move from point 1 to point 2
	time="0" 					// how far though the movement it is. egset to "duration / 2" would mean its half way to point 2
	
	rotating="0" 				// indicates if the laser rotates or not (0=NotRotation, 1=Rotating)
	start_angle="0" 			// the start angle of the laser
	delta_angle="45" 			// how much the laser rotates
	rotation_duration="1" 		// how long it takes to laser to rotation the delta_angle amount
	rotation_time="0"			// how far through the rotation it is
	/>

	-->

	<!-- Top Row Lasers -->
	<object type="laser" position="-1.6,3" laser_direction="0,1" p1="0,0" p2="5,0" moving="0" duration="5" time="0" start_angle="0" delta_angle="45" rotation_duration="1" rotating="0" rotation_time="0"/>
	<object type="laser" position="-0.2,3" laser_direction="0,1" p1="0,0" p2="5,0" moving="0" duration="5" time="0" start_angle="0" delta_angle="45" rotation_duration="1" rotating="0" rotation_time="0"/>
	<object type="laser" position="1.2,3" laser_direction="0,1" p1="0,0" p2="5,0" moving="0" duration="5" time="0" start_angle="0" delta_angle="45" rotation_duration="1" rotating="0" rotation_time="0"/>
	<object type="laser" position="2.6,3" laser_direction="0,1" p1="0,0" p2="5,0" moving="0" duration="5" time="0" start_angle="0" delta_angle="45" rotation_duration="1" rotating="0" rotation_time="0"/>
	
	<!-- Bottom Row Lasers -->
	<object type="laser" position="-1.6,-3" laser_direction="0,-1" p1="0,0" p2="5,0" moving="0" duration="5" time="0" start_angle="0" delta_angle="45" rotation_duration="1" rotating="0" rotation_time="0"/>
	<object type="laser" position="-0.2,-3" laser_direction="0,-1" p1="0,0" p2="5,0" moving="0" duration="5" time="0" start_angle="0" delta_angle="45" rotation_duration="1" rotating="0" rotation_time="0"/>
	<object type="laser" position="1.2,-3" laser_direction="0,-1" p1="0,0" p2="5,0" moving="0" duration="5" time="0" start_angle="0" delta_angle="45" rotation_duration="1" rotating="0" rotation_time="0"/>
	<object type="laser" position="2.6,-3" laser_direction="0,-1" p1="0,0" p2="5,0" moving="0" duration="5" time="0" start_angle="0" delta_angle="45" rotation_duration="1" rotating="0" rotation_time="0"/>
	
	<!-- Center Laser -->
	<object type="laser" position="6,2" laser_direction="0,-1" p1="0,0" p2="5,0" moving="0" duration="5" time="0" start_angle="0" delta_angle="45" rotation_duration="1" rotating="0" rotation_time="0"/>
	

	<!-- WHERE THE HOLE IS ON THE LEVEL-->
	<object type="hole" position="-7.5,-2" />
	<!-- WHERE THE PLAYER STARTS THE LEVEL-->
	<object type="player" position="-7.5,2" />
	
	<!-- BARRIERS in the middle + connection onto the outer frame-->
	<object type="barrier" position="1.25,0" size="8.5,6" rotation="0"/>
	<object type="barrier" position="6,2.5" size="1,1" rotation="0"/>
	<object type="barrier" position="6,-2.5" size="1,1" rotation="0"/>
	<object type="barrier" position="-4,0" size="2,6" rotation="0"/>
	<object type="barrier" position="-7.5,0" size="5,2" rotation="0"/>
	
	<!-- Outter frame clockwise from farleft -->
	<object type="barrier" position="-14.5,0" size="9,21" rotation="0"/>
	<object type="barrier" position="1,9.25" size="22,2.5" rotation="0"/>
	<object type="barrier" position="15.5,0" size="7,21" rotation="0"/>
	<object type="barrier" position="1,-9.25" size="22,2.5" rotation="0"/>
	<object type="barrier" position="12,8" size="6,6" rotation="-45"/>
	<object type="barrier" position="12,-8" size="6,6" rotation="45"/>
	
	<!-- Moving Platforms [MIDDLE, TOP, BOTTOM]-->
	<object type="block" position="0,0" size="0.4,2" rotation="0" moves="1" p1="6.9,2" p2="6.9,-2" duration="2" time="0" />
	<object type="block" position="0,0" size="4,0.4" rotation="0" moves="1" p1="-3,3.5" p2="4,3.5" duration="2" time="0" />
